<%- include('includes/header.ejs') %>


  <% persones.forEach( persone => { %>

    <h1>Name: <%= persone.name %></h1>
    <h2>Age: <%= persone.age %></h2>
    <h2>Address: </h2>
    <ul>
        <% persone.address.forEach( add => { %>
            <li>City: <%= add.city %></li>
            <li>street: <%= add.street %></li> 
            <h5>Phones:</h5>
            <ul>
              <% add.phones.forEach(phone => { %>
                  <li>home: <%= phone.home %> : mobile: <%= phone.mobile %> </li>
             <% }) %>

        <% }) %>
        
      </ul>
    </ul>
 <% }) %>
</body>
</html>
