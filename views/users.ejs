<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% users.forEach(user => { %>
      <h2><%= user.username%> <%= user.lastname%></h2>
      <div>
        <a href="/deleteUser/<%= user._id%>">Delete</a>
        <a href="/update/<%= user._id%>">Update</a>
      </div>
    <% }) %>

<br>
<br>
<br>
<br>
    <a href="/create">Create New User</a>
  </body>
</html>
